<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">
	<groups>
		<run>
			<include name="smoke"/>
			<include name="sanity"/>
			<exclude name="regerssion"></exclude>
		</run>
	</groups>
	
	<test thread-count="2" name="Smoke">
		<classes>
			<class name="testNG.TestNG_Group1"></class>
		</classes>
	</test>
  <test thread-count="2" name="Sanity">
    <classes>
      <class name="testNG.TestNG_Group2"/>
    </classes>
  </test> <!-- Test -->
</suite> <!-- Suite -->
